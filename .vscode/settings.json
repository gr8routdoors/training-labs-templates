{
    "cmake.configureOnOpen": false,
    "debug.console.fontFamily": "D2Coding",
    "debug.console.fontSize": 13,
    "editor.columnSelection": false,
    "editor.cursorStyle": "block",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "Fantasque Sans Mono, JetBrains Mono, Oxygen Mono, D2Coding, Consolas, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.fontWeight": "600",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.lineHeight": 18,
    "editor.minimap.enabled": true,
    "editor.mouseWheelZoom": true,
    "editor.suggestSelection": "first",
    "editor.wordWrapColumn": 120,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "java.format.enabled": false,
    "java.home": "/usr/local/opt/openjdk@11",
    "java.semanticHighlighting.enabled": true,
    "material-icon-theme.showWelcomeMessage": false,
    "materialTheme.accent": "Orange",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.symbolsHierarchyDepthLimit": 15,
    "terminal.integrated.fontFamily": "Fantasque Sans Mono, D2Coding, JetBrains Mono, Consolas, Cousine, Menlo, monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "500",
    "terminal.integrated.lineHeight": 1.05,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.autoDetectColorScheme": true,
    "window.zoomLevel": 1.0,
    "workbench.colorTheme": "Monokai Pro",
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.panel.defaultLocation": "right",
    "workbench.preferredDarkColorTheme": "Monokai Pro",
    "workbench.preferredLightColorTheme": "Ayu Light",
    "workbench.tree.renderIndentGuides": "always",
    "files.associations": {
        "*.bash": "shellscript",
        "*.sh": "shellscript",
        ".bazelrc.user": "bazelrc",
        "user.bazelrc": "bazelrc",
        "*.md": "markdown"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.exclude": {
        "bazel-*": false,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
}